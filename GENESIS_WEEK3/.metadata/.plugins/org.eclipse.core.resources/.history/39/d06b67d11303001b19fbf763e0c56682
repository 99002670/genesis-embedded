#include "STM32F407xx_gpio_driver.h"

/**************************** API DEFINITIONS *******************************/
void GPIO_Peri_CLKControl(GPIO_REGDEF_t *pGPIOx, uint8_t EorDi)
{
	if(pGPIOx == GPIOA)
	{
		RCC GPIOA_CLK_EN();
	}
	else if (pGPIOx == GPIOB)
	{
		RCC GPIOB_CLK_EN();
	}
}
